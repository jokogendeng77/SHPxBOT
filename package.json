{
  "type": "module",
  "name": "shopee-app",
  "bin": "./SHPxBOT.js",
  "main": "SHPxBOT.js",
  "scripts": {
    "start": "node SHPBOT.js",
    "build": "npm run pre-build && npm run build-exe",
    "pre-build": "esbuild _jangkrik.js --bundle --platform=node --target=node20.9.0 --outfile=SHPxBOT.js --packages=external",
    "build-tt": "tsc",
    "build-exe": "pkg . -t node18-win-x64 -o dist/SHPBOT.exe"
  },
  "pkg": {
    "assets": [
      "config/*",
      "produkcsv/*",
      "cookieShopee.txt",
      "stream_output/*",
      "license.txt",
      "TUTORIAL.md"
    ],
    "scripts":["library/*.cjs"],
    "targets": [
      "node20-win-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "ascii-art": "^2.8.5",
